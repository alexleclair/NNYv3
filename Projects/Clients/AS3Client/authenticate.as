package{	public class authenticate	{		/*		 *	@params		str:String, secret:String		 *	@desc		encodes the given string using the given secret. This method is safe to put in a string, since it only outputs printable characters!		 *	@returns	String		 */		public static function getResponse(str:String, secret:String):String		{			var reply:String="";			var leftLength:int  = str.length;			var rightLength:int = secret.length;			for(var i:int=0; i<leftLength; ++i)				reply+= String.fromCharCode((str.charCodeAt(i) ^ secret.charCodeAt(i%rightLength))%93+33);			return reply;		}	}}